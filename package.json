{
  "name": "getthemall",
  "license": "MIT",
  "version": "3.0.1",
  "author": "Vladimir Kovpak",
  "description": "Super simple package built for demo purposes.",
  "keywords": [
    "nodejs",
    "javascript",
    "rest-api",
    "json",
    "node-fetch",
    "mocha",
    "chai",
    "nock",
    "ecmascript6"
  ],
  "homepage": "https://github.com/cn007b/getthemall#readme",
  "bugs": {
    "url": "https://github.com/cn007b/getthemall/issues"
  },
  "main": "./src/index.js",
  "engines": {
    "node": "8.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cn007b/getthemall.git"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint src/",
    "test": "npm run utest && npm run itest",
    "utest": "node_modules/mocha/bin/_mocha --recursive --reporter spec test/unit",
    "itest": "node_modules/mocha/bin/_mocha --recursive --reporter spec test/integration",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nock": "^9.0.13"
  },
  "dependencies": {
    "node-fetch": "^1.7.1",
    "valid-url": "^1.0.9"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "globals": {},
    "plugins": [],
    "extends": [
      "airbnb-base"
    ]
  }
}
